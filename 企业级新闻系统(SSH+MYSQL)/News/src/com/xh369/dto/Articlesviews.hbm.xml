<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.xh369.dto.Articlesviews" table="articlesviews">
        <composite-id name="id" class="com.xh369.dto.ArticlesviewsId">
            <key-property name="id" type="int">
                <column name="id" />
            </key-property>
            <key-property name="title" type="string">
                <column name="title"/>
            </key-property>
            <key-property name="bigclassname" type="string">
                <column name="bigclassname"/>
            </key-property>
            <key-property name="smallclassname" type="string">
                <column name="smallclassname"/>
            </key-property>
            <key-property name="deploytime" type="timestamp">
                <column name="deploytime"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
